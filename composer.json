{
  "name": "zacksleo/yii2-wechat",
  "description": "Yii2 wechat module",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "wechat",
    "module"
  ],
  "license": "BSD-3-Clause",
  "authors": [{
    "name": "zacksleo",
    "email": "zacksleo@gmail.com"
  }],
  "require": {
    "yiisoft/yii2": "*",
    "yiisoft/yii2-bootstrap": "*",
    "pheme/yii2-settings": "^0.6",
    "kartik-v/yii2-tree-manager": "^1.0",
    "npm-asset/font-awesome": "^4.7",
    "overtrue/wechat": "^4.0",
    "jianyan74/yii2-easy-wechat": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.4"
  },
  "autoload": {
    "psr-4": {
      "zacksleo\\yii2\\wechat\\": "src"
    }
  },
  "repositories": [{
    "type": "composer",
    "url": "https://asset-packagist.org"
  }],
  "extra": {
    "yii\\composer\\Installer::postInstall": {
      "copyFiles": [{
        "vendor/zacksleo/yii2-wechat/src/assets": "frontend/web/"
      }]
    }
  }
}